{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parser": "babel-eslint",
	"extends": ["react-app", "airbnb", "prettier", "prettier/react"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"settings": {
		"import/resolver": {
			"node": {
				"paths": ["src"]
			}
		}
	},
	"rules": {
		// "prettier/prettier": [
		//   "warn",
		//   {
		//     "endOfLine": "auto",
		//     "printWidth": 120,
		//     "singleQuote": true,
		//     "tabWidth": 4,
		//     "useTabs": true,
		//     "trailingComma": "none",
		//     "arrowParens": "avoid"
		//   }
		// ],
		"react/jsx-filename-extension": [
			"warn",
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"global-require": "off",
		"import/prefer-default-export": "off",
		"no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
	}
}
